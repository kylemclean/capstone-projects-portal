{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.51.3",
    "react-multi-carousel": "^2.8.5",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^5.3.4",
    "react-share": "^5.1.0",
    "react-tabs": "^6.0.2",
    "react-tag-input": "^6.9.0",
    "vite": "^5.2.8",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "npm run-script test:unit && npm run-script test:e2e",
    "test:unit": "vitest --run",
    "test:e2e": "playwright test",
    "lint-staged": "lint-staged",
    "precommit": "cd .. && npm run-script precommit",
    "reset-database": "cd .. && npm run-script reset-database",
    "install-all-dependencies": "cd .. && npm run-script install-all-dependencies",
    "check-types": "cd .. && npm run-script check-types",
    "check-format": "cd .. && npm run-script check-format",
    "check-lint": "cd .. && npm run-script check-lint",
    "format-prettier": "cd .. && npm run-script format-prettier",
    "format-pipenv": "cd .. && npm run-script format-pipenv",
    "check-frontend": "cd .. && npm run-script check-frontend",
    "check-backend": "cd .. && npm run-script check-backend",
    "check-all": "cd .. && npm run-script check-all",
    "check-format-all": "cd .. && npm run-script check-format-all",
    "test-frontend": "cd .. && npm run-script test-frontend",
    "test-backend": "cd .. && npm run-script test-backend",
    "test-all": "cd .. && npm run-script test-all",
    "start-frontend": "cd .. && npm run-script start-frontend",
    "start-backend": "cd .. && npm run-script start-backend",
    "start-servers": "cd .. && npm run-script start-servers"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/chromedriver": "^81.0.5",
    "@types/node": "^22.15.17",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-tag-input": "^6.6.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "chromedriver": "^123.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "tsc-files": "^1.1.4",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "tsc-files --noEmit",
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": "prettier --write"
  }
}
