{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@hookform/resolvers": "^2.8.2",
        "@mui/icons-material": "^5.0.1",
        "@mui/lab": "^5.0.0-alpha.50",
        "@mui/material": "^5.0.2",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "axios": "^0.22.0",
        "react": "^17.0.2",
        "react-dnd": "^14.0.4",
        "react-dnd-html5-backend": "^14.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-hook-form": "^7.17.1",
        "react-multi-carousel": "^2.6.5",
        "react-responsive-carousel": "^3.2.22",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.3",
        "react-share": "^4.4.0",
        "react-tabs": "^3.2.3",
        "react-tag-input": "^6.7.3",
        "web-vitals": "^1.1.2",
        "yup": "^0.32.9"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "test": "npm run-script test:unit && npm run-script test:e2e",
        "test:unit": "react-scripts test --testPathIgnorePatterns=/e2e-tests/",
        "test:e2e": "start-server-and-test test:e2e-start http://localhost:3356 test:e2e-run-tests",
        "test:e2e-start": "cross-env REACT_APP_API_BASE_URL=http://localhost:8192/api REACT_APP_GITHUB_AUTHORIZE_URL=http://localhost:8192/api/mock-github/login/oauth/authorize BROWSER=none PORT=3356 react-scripts start",
        "test:e2e-run-tests": "react-scripts test --testPathPattern=/e2e-tests/ --testTimeout=60000 --runInBand",
        "lint-staged": "lint-staged",
        "precommit": "cd .. && npm run-script precommit",
        "prepare": "cd .. && npm run-script prepare",
        "reset-database": "cd .. && npm run-script reset-database",
        "install-all-dependencies": "cd .. && npm run-script install-all-dependencies",
        "check-types": "cd .. && npm run-script check-types",
        "check-format": "cd .. && npm run-script check-format",
        "check-lint": "cd .. && npm run-script check-lint",
        "format-prettier": "cd .. && npm run-script format-prettier",
        "format-pipenv": "cd .. && npm run-script format-pipenv",
        "check-frontend": "cd .. && npm run-script check-frontend",
        "check-backend": "cd .. && npm run-script check-backend",
        "check-all": "cd .. && npm run-script check-all",
        "check-format-all": "cd .. && npm run-script check-format-all",
        "test-frontend": "cd .. && npm run-script test-frontend",
        "test-backend": "cd .. && npm run-script test-backend",
        "test-all": "cd .. && npm run-script test-all",
        "start-frontend": "cd .. && npm run-script start-frontend",
        "start-backend": "cd .. && npm run-script start-backend",
        "start-servers": "cd .. && npm run-script start-servers"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/chromedriver": "^81.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^26.0.24",
        "@types/material-ui": "^0.21.9",
        "@types/node": "^12.20.27",
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.9",
        "@types/react-helmet": "^6.1.4",
        "@types/react-router-dom": "^5.3.0",
        "@types/react-tabs": "^2.3.3",
        "@types/react-tag-input": "^6.1.3",
        "@types/selenium-webdriver": "^4.0.16",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "chromedriver": "^96.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "express": "^4.17.1",
        "lint-staged": "^11.1.2",
        "msw": "^0.35.0",
        "prettier": "^2.4.1",
        "selenium-webdriver": "^4.0.0",
        "start-server-and-test": "^1.14.0",
        "tsc-files": "^1.1.2",
        "typescript": "^4.4.3"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": "tsc-files --noEmit",
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix --max-warnings 0",
            "prettier --write"
        ],
        "src/**/*.{json,css,scss,md}": "prettier --write"
    }
}
